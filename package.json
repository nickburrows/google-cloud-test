  "scripts": {
    "deploy": "gcloud functions deploy leafrainsuns --allow-unauthenticated --entry-point=helloGET --env-vars-file=.env.yaml --gen2 --max-instances=1 --memory=128Mi --no-user-output-enabled --region=asia-east1 --runtime=nodejs20 --timeout=60s --trigger-http && gcloud run services update leafrainsuns --region=asia-east1 --cpu 1 --concurrency 80",
